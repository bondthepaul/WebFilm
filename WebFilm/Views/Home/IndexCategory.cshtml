@model IEnumerable<WebFilm.Models.Category>

@{
    ViewBag.Title = "IndexCategory";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Danh mục film</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Catename)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Catename)
        </td>
        <td>
            @Html.ActionLink("Vào trang", "Index", new { categoryID = item.CategoryID }) 
          
        </td>
    </tr>
}

</table>
